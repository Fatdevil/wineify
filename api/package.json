{
  "name": "api",
  "version": "1.0.0",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "lint": "eslint \"src/**/*.{js,ts}\" --ignore-path .gitignore",
    "test": "NODE_ENV=test jest --config jest.config.js",
    "test:coverage": "NODE_ENV=test jest --coverage --config jest.config.js",
    "test:watch": "NODE_ENV=test jest --watch --config jest.config.js",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "build": "tsup --clean",
    "dev": "tsx watch --env-file=.env --env-file=.env.local src/index.ts",
    "start": "node --env-file=.env --env-file=.env.local dist/index.js",
    "typecheck": "tsc --noEmit",
    "prepare": "npm run build",
    "db:generate": "prisma generate",
    "db:validate": "prisma validate",
    "db:migrate": "prisma migrate dev",
    "db:deploy": "prisma migrate deploy",
    "db:seed": "ts-node --transpile-only --compiler-options '{\"module\":\"commonjs\",\"esModuleInterop\":true}' prisma/seed.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@prisma/client": "^6.16.2",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "pino": "^9.10.0",
    "pino-http": "^10.5.0",
    "zod": "^4.1.9"
  },
  "devDependencies": {
    "@types/bcryptjs": "^3.0.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.5.1",
    "@types/supertest": "^6.0.3",
    "jest": "^30.1.3",
    "nodemon": "^3.1.10",
    "prisma": "^6.16.2",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  }
}
